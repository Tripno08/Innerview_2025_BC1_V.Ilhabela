{
  "name": "innerview-ilhabela-backend",
  "version": "1.0.0",
  "description": "Backend para o projeto Innerview Ilhabela",
  "main": "dist/server.js",
  "scripts": {
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "build": "tsc -p tsconfig.build.json",
    "build:ignore-errors": "tsc --skipLibCheck --skipDefaultLibCheck --noEmitOnError false -p tsconfig.build.json",
    "build:force": "tsc --skipLibCheck --skipDefaultLibCheck --noEmit false --noEmitOnError false -p tsconfig.build.json",
    "build:manual": "tsc-silent -p tsconfig.build.json --suppress '.*/.*' && tsc-alias",
    "build:permissive": "./scripts/build-permissive.sh",
    "test": "jest",
    "test:unit": "jest --testMatch='**/*.unit.test.ts'",
    "test:integration": "jest --testMatch='**/*.integration.test.ts'",
    "test:coverage": "jest --coverage",
    "dev": "ts-node-dev -r tsconfig-paths/register src/server.ts",
    "start": "node dist/server.js"
  },
  "keywords": [
    "clean-architecture",
    "typescript",
    "node",
    "express",
    "prisma"
  ],
  "dependencies": {
    "@prisma/client": "^5.9.0",
    "bcryptjs": "^2.4.3",
    "celebrate": "^15.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.6.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "reflect-metadata": "^0.2.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tsyringe": "^4.8.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^18.19.4",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "codecov": "^3.8.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "lint-staged": "^15.2.0",
    "prettier": "^3.5.3",
    "prisma": "^5.9.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.11",
    "tsc-silent": "^1.2.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
